{
  "name": "jenkins-demo",
  "version": "1.0.0",
  "description": "Simple calculator application with tests",
  "main": "src/app.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "node src/app.js",
    "test": "jest --testMatch='<rootDir>/tests/**/*.test.js'",
    "test:ui": "jest --testMatch='<rootDir>/ui-tests/**/*.test.js'"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com-personal:fu-ry17/jenkins-ansible.git"
  },
  "dependencies": {
    "express": "4.18.2",
    "semver": "7.5.4"
  },
  "devDependencies": {
    "jest": "29.7.0",
    "puppeteer": "22.4.1",
    "supertest": "7.0.0"
  },
  "engines": {
    "node": ">=22.11.0",
    "npm": ">=10.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "jest": {
    "testMatch": [
      "<rootDir>/tests/**/*.test.js"
    ],
    "testEnvironment": "node"
  }
}
